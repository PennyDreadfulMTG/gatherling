{
    "name": "pennydreadfulmagic/gatherling",
    "description": "Magic the Gathering tournament software",
	"version": "5.1.0",
    "require": {
        "php": "8.1.*",
        "ext-curl": "*",
        "ext-mysqli": "*",
        "ext-pdo": "*",
        "ext-pdo_mysql": "*",
        "ext-simplexml": "*",
        "wohali/oauth2-discord-new": "^1.0",
        "sentry/sdk": "^4.0",
        "ypho/scryfall": "*",
        "firebase/php-jwt": "^6.10",
        "mustache/mustache": "^2.14"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "3.*",
        "phpunit/phpunit": "^9.5",
        "phpcompatibility/php-compatibility": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Gatherling\\Models\\": "gatherling/models/",
            "Gatherling\\Pages\\": "gatherling/pages/",
            "": "gatherling/"
        }
    }
}
