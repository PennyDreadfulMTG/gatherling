{
    "name": "pennydreadfulmagic/gatherling",
    "description": "Magic the Gathering tournament software",
	"version": "5.1.0",
    "scripts": {
        "test": "vendor/bin/phpunit tests",
        "lint": "vendor/bin/phpcs --standard=PSR12 --runtime-set testVersion 8.1 --ignore=vendor .",
        "autofix": "vendor/bin/phpcbf --standard=PSR12 --runtime-set testVersion 8.1 --ignore=vendor ."
    },
    "require": {
        "php": "8.1.*",
        "ext-curl": "*",
        "ext-mysqli": "*",
        "ext-pdo": "*",
        "ext-pdo_mysql": "*",
        "ext-simplexml": "*",
        "wohali/oauth2-discord-new": "^1.0",
        "sentry/sdk": "^4.0",
        "ypho/scryfall": "*",
        "firebase/php-jwt": "^6.10",
        "mustache/mustache": "^2.14",
        "monolog/monolog": "^3.7"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "3.*",
        "phpunit/phpunit": ">=10.0",
        "phpcompatibility/php-compatibility": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Gatherling\\": "gatherling/",
            "Gatherling\\Tests\\": "tests/"
        }
    }
}
